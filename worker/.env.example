# Worker ID (必須)
WORKER_ID=aws-tyo1a

# Supabase URL (必須)
SUPABASE_URL=https://your-project-id.supabase.co

# Pingmon Worker API Key (必須)
PINGMON_WORKER_API_KEY=your-api-key-here

# キュー戦略 (オプション)
QUEUE_STRATEGY=weighted

# 最大同時実行数 (オプション)
MAX_CONCURRENT_CHECKS=50

# 監視設定再読み込み間隔 (オプション)
RELOAD_INTERVAL_MS=300000

# 強制キャンセルバッファ時間（秒） (オプション)
# タスクが timeout_seconds + この秒数を超えたら強制キャンセル
FORCE_CANCEL_BUFFER_SECONDS=10
